{
  "version": 1,
  "plugins": {
    "namespaces": true,
    "landlock": true,
    "seccomp": true
  },
  "defaults": {
    "fs": "deny",
    "net": "allow",
    "exec": "deny"
  },
  "net": {
    "rules": [
      {
        "action": "deny",
        "proto": "tcp",
        "cidr": "169.254.169.254/32",
        "ports": "80,443"
      },
      {
        "action": "deny",
        "proto": "tcp",
        "cidr": "169.254.0.0/16",
        "ports": "0-65535"
      },
      {
        "action": "deny",
        "proto": "tcp",
        "cidr": "10.0.0.0/8",
        "ports": "0-65535"
      },
      {
        "action": "deny",
        "proto": "tcp",
        "cidr": "172.16.0.0/12",
        "ports": "0-65535"
      },
      {
        "action": "deny",
        "proto": "tcp",
        "cidr": "192.168.0.0/16",
        "ports": "0-65535"
      },
      {
        "action": "warn",
        "proto": "tcp",
        "cidr": "0.0.0.0/0",
        "ports": "2222"
      },
      {
        "action": "warn",
        "proto": "tcp",
        "cidr": "0.0.0.0/0",
        "ports": "25"
      },
      {
        "action": "warn",
        "proto": "tcp",
        "cidr": "0.0.0.0/0",
        "ports": "3306"
      },
      {
        "action": "warn",
        "proto": "tcp",
        "cidr": "0.0.0.0/0",
        "ports": "5432"
      },
      {
        "action": "warn",
        "proto": "tcp",
        "cidr": "0.0.0.0/0",
        "ports": "6379"
      }
    ]
  }
}