{
  "version": 1,
  "plugins": {
    "namespaces": true,
    "landlock": true,
    "seccomp": true
  },
  "defaults": {
    "fs": "deny",
    "net": "deny",
    "exec": "deny"
  },
  "fs": {
    "rules": [
      {
        "action": "warn",
        "path": "/tmp/bench.txt",
        "perms": ["read_file", "write_file"]
      }
    ]
  }
}
